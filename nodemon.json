{
    "ignore": ["build", "example/*", "test/*", "docs/*"],
    "delay": "100",
    "watch": [
        "./index.ts",
        "./src/*",
        "./tsconfig.dev.json",
        "./tsconfig.json",
        "./tsling.json",
        "./.prettierrc.json",
        "./.prettierignore"
    ],
    "exec": ["npm run build:dev:nodemon"],
    "ext": "ts, json"
}
